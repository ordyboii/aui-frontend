{
  "pluginDependencies": [
    {
      "packageName": "govuk-frontend",
      "minVersion": "5.7.1"
    }
  ],
  "assets": ["/assets/", "/images/profile.png"],
  "stylesheets": ["/aui.css"],
  "scripts": ["/aui.js"],
  "importNunjucksMacrosInto": [
    "/aui/layouts/aui.njk",
    "/aui/layouts/aui-with-sidebar.njk",
    "/aui/layouts/aui-with-case.njk"
  ],
  "templates": [
    {
      "path": "/aui/templates/aui.njk",
      "name": "Adviser UI page",
      "type": "nunjucks"
    },
    {
      "path": "/aui/templates/aui-with-sidebar.njk",
      "name": "Adviser UI page with main sidebar",
      "type": "nunjucks"
    },
    {
      "path": "/aui/templates/aui-with-case.njk",
      "name": "Adviser UI page with co-existence Pega Constellation case",
      "type": "nunjucks"
    }
  ],
  "nunjucksPath": ["/"],
  "nunjucksMacros": [
    {
      "macroName": "auiHeader",
      "importFrom": "aui/components/aui-header/macro.njk"
    },
    {
      "macroName": "auiNavigation",
      "importFrom": "aui/components/aui-navigation/macro.njk"
    },
    {
      "macroName": "auiInformationBanner",
      "importFrom": "aui/components/aui-information-banner/macro.njk"
    },
    {
      "macroName": "auiSidebar",
      "importFrom": "aui/components/aui-sidebar/macro.njk"
    },
    {
      "macroName": "auiModal",
      "importFrom": "aui/components/aui-modal/macro.njk"
    },
    {
      "macroName": "auiPopover",
      "importFrom": "aui/components/aui-popover/macro.njk"
    },
    {
      "macroName": "auiConstellationCase",
      "importFrom": "aui/components/aui-constellation-case/macro.njk"
    },
    {
      "macroName": "auiConstellationMetaList",
      "importFrom": "aui/components/aui-constellation-meta-list/macro.njk"
    },
    {
      "macroName": "auiConstellationFieldValueList",
      "importFrom": "aui/components/aui-constellation-field-value-list/macro.njk"
    },
    {
      "macroName": "auiConstellationBanner",
      "importFrom": "aui/components/aui-constellation-banner/macro.njk"
    }
  ]
}
